{
  "name": "ninja-url",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "dotenv -e .env.local -- tsx scripts/seed.ts",
    "db:link:dev": "supabase link --project-ref njvjlzmflamitrqrxgib",
    "db:link:prod": "supabase link --project-ref gcfknnukbzzfjgafmfxj",
    "db:generate": "npx supabase gen types typescript --project-id 'njvjlzmflamitrqrxgib' > ./src/lib/supabase/types.ts",
    "db:migrate": "supabase db diff -f",
    "db:push:dev": "pnpm run db:link:dev && supabase db push",
    "db:push:prod": "pnpm run db:link:prod && supabase db push",
    "db:setup": "supabase stop --no-backup && source ./supabase/.env && supabase start && supabase db reset && pnpm run seed && pnpm run db:generate",
    "db:restart": "supabase stop --no-backup && source ./supabase/.env && supabase start"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.512.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.3",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "supabase": ">=1.8.1",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.3.3",
    "typescript": "^5"
  }
}
